<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>


<HBox alignment="CENTER" spacing="5.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox prefWidth="200.0" HBox.hgrow="NEVER">
         <children>
            <Label fx:id="itemNameLabel" maxHeight="1.7976931348623157E308" maxWidth="200.0" minHeight="-Infinity" minWidth="-Infinity" prefWidth="200.0" />
         </children>
      </HBox>
      <HBox prefWidth="16.0" HBox.hgrow="NEVER">
         <children>
            <CheckBox fx:id="checkBox" mnemonicParsing="false" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets left="5.0" right="5.0" />
   </padding>
</HBox>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" stylesheets="@style.css" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox spacing="5.0">
         <children>
            <ImageView fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="NEVER">
               <image>
                  <Image url="@img/teriklee.png" />
               </image>
            </ImageView>
            <VBox maxWidth="400.0" prefHeight="150.0" prefWidth="400.0" spacing="5.0" HBox.hgrow="NEVER">
               <children>
                  <Label fx:id="titleLabel" maxHeight="1.7976931348623157E308" maxWidth="400.0" minHeight="-Infinity" minWidth="-Infinity" prefWidth="380.0" styleClass="update-title" wrapText="true" />
                  <Label fx:id="contentLabel" maxHeight="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefWidth="380.0" styleClass="error-content" wrapText="true" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
      <HBox alignment="CENTER_RIGHT" spacing="5.0">
         <children>
            <Button mnemonicParsing="false" onAction="#handleOk" prefHeight="25.0" prefWidth="100.0" text="OK" />
            <Button fx:id="goButton" mnemonicParsing="false" onAction="#handleGo" prefHeight="25.0" prefWidth="100.0" styleClass="button-action-color" text="GO" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
